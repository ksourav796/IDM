package com.hyva.idm.sass.sassmapper;

import com.hyva.idm.sass.sassentities.PktFields;
import com.hyva.idm.sass.sassentities.PktPermission;
import com.hyva.idm.sass.sasspojo.PktFieldsPojo;
import com.hyva.idm.sass.sasspojo.PktPermissionPojo;

/**
 * Created by krishna.
 */
public class PktFieldMapper {

    public static PktFields mapPktFieldPojoToEnity(PktFieldsPojo pktFieldsPojo){
        PktFields pktFields  = new PktFields();
        pktFields.setId(pktFieldsPojo.getId());
        pktFields.setTableName(pktFieldsPojo.getTableName());
        pktFields.setFieldName(pktFieldsPojo.getFieldName());
        pktFields.setGroupOf(pktFieldsPojo.getGroupOf());
        pktFields.setStatus(pktFieldsPojo.getStatus());
        return  pktFields;
    }
}
