                                    <tr>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Company Name</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Active</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Header</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Footer</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Edit</div>
                                        </th>

                                    </tr>
                                    <tbody id="parentTable">
                                    <tr ng-repeat="menu in menuList | unique: 'applicationName'">
                                        <td>{{menu.applicationName}}</td>
                                        <td>{{menu.status}}</td>
                                        <td><a id="operation_{{$index}}" href="javascript:void(0)"
                                               ng-click="getHeaderValuesList(menu)" class="operation_"><i
                                                class="fa fa-pencil-square-o fa-fw orange"></i></a></td>
                                        <td><a id="edit_{{$index}}" href="javascript:void(0)"
