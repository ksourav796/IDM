                    <div class="modal fade ExchangeRate" tabindex="-1"   id="add_new_pkt_Footer_modal" role="dialog"
                         data-backdrop="static"
                         aria-labelledby="mySmallModalLabel" style="overflow: auto;">
                        <div class="modal-dialog modal-md">
                            <div class="modal-content checkout">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                            aria-hidden="true">&times;</span></button>
                                    <h4 id="cust-notif" class="modal-title">Add Footer</h4>
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-md-12 col-sm-12">
                                            <form class="form-horizontal" name="table">
                                                <fieldset>



                                                    <div class="form-group">
                                                        <label class="col-md-4 col-sm-4 control-label" for=""> Footer</label>
                                                        <div class="col-md-6 col-sm-6" style="position:relative;">
                                                            <input id="footer" name="footer"
                                                                   type="text" placeholder="Footer"
                                                                   ng-model="footer"
                                                                   class="form-control input-md">

                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-4 col-sm-4 control-label" for="">Footer Value</label>
                                                        <div class="col-md-6 col-sm-6" style="position:relative;">
                                                            <input id=" footervalue" name="footervalue"
                                                                   type="text" placeholder="Footer Value "
                                                                   ng-model="footervalue"
                                                                   ng-click="menubuilderfooterlist()"
                                                                   class="form-control input-md" autocomplete="off">

                                                        </div>
                                                    </div>


                                                </fieldset>
                                                <div class="modal-footer">
                                                    <button type="submit" class="btn btn-default" data-toggle="modal"
                                                            data-target=".print-me" ng-disabled="isDisabled" ng-click="savefooter()">Save
                                                    </button>
                                                    <button type="button" class="btn btn-default"
                                                            ng-click="removetableType()" data-dismiss="modal">close
                                                    </button>

                                                </div>

                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="modal fade " tabindex="-1" id="add_new_Pkt_Image_modal" role="dialog"
                         data-backdrop="static"
                         aria-labelledby="mySmallModalLabel">
                        <div class="modal-dialog modal-md">
                            <div class="modal-content checkout">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                            aria-hidden="true">&times;</span></button>
                                    <h4 id="cust-notif">Add Image</h4>
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-md-12 col-sm-12">
                                            <form class="form-horizontal" name="mail">
                                                <fieldset>
                                                    <div class="form-group">
                                                        <div class="form-group">
                                                            <div class="col-xs-6 col-xs-offset-2">
                                                                <input type='file' accept='image/*' id="imageLoad" ng-model-instant
                                                                       onchange="angular.element(this).scope().imageUpload(event)"><br>
                                                                <img class="image-preview"
                                                                     style="width:200px;height:200px; margin-left: 39px;" >
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="col-md-3 col-sm-3 control-label" for="">Description</label>
                                                            <div class="col-md-6 col-sm-6" style="position:relative;">
                                                                <textarea id="Description" name="Description"
                                                                          type="text" placeholder="Description"
                                                                          ng-model="description"
                                                                          class="form-control input-md" autocomplete="off"></textarea>

                                                            </div>
                                                        </div>

                                                        <div class="modal-footer">
                                                            <button type="submit" class="btn btn-default" data-toggle="modal"
                                                                    data-target=".print-me" ng-click="saveProperty()">save
                                                            </button>
                                                            <button type="button" class="btn btn-default"
                                                                    ng-click="removeAction()" data-dismiss="modal">close
                                                            </button>

                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
