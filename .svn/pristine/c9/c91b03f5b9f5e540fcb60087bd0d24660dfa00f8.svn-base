<!DOCTYPE html>
<html lang="en">

<body>
<div class="container-fluid">
    <div class="container cus_main_row">
        <div class="row">
            <h2 class="pos-header-new">
                <div>PktUser</div>
            </h2>
        </div>
        <div class="row inner_row_bg">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="panel">
                    <div class="panel-body" style="height: 65px;">
                        <div class="col-md-4 col-lg-3 col-sm-5 col-xs-5">
                            <form class="form-horizontal ng-pristine ng-valid">
                                <div class="form-group">
                                    <div class="col-sm-12">

                                    </div>
                                </div>
                            </form>
                        </div>
                        <!--<div class="col-lg-7 col-md-9 col-sm-9">-->
                            <div class="form-group">
                                <div class="col-sm-2">
                                    <button id="" type="button" class="btn btn-default hi_btn"
                                            ng-click="addpktuser()">Add
                                    </button>
                                </div>
                            </div>
                        <!--</div>-->
                    </div>
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="fixed-table-container">
                            <div class="table_wrap fixed-table-container-inner">
                                <div class="header-background"></div>
                                <table data-toggle="table" data-cache="false" data-height="299"
                                       style="margin-bottom: 0px;"
                                       class="hi_table table table-hover table-bordered table-striped table-fixed-header">
                                    <thead>
                                    <tr>
                                        <th data-field="formRegno">
                                            <div class="th-inner" style="text-align: center">UserName</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner" style="text-align: center">Email</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner" style="text-align: center">Application</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner" style="text-align: center">Edit</div>
                                        </th>


                                    <tbody id="parentTable">
                                    <tr ng-repeat="pktuser in pktuserlist| filter:searchText">
                                        <td>{{pktuser.userName}}</td>
                                        <td>{{pktuser.email}}</td>
                                        <td>{{pktuser.application}}</td>
                                        <td><a id="edit_{{$index}}" href="javascript:void(0)" ng-click="editPktUser(pktuser)" class="edit"><i class="fa fa-pencil-square-o fa-fw orange"></i></a></td>

                                        <!--<td><a id="edit_{{$index}}" href="javascript:void(0)"-->
                                        <!--ng-click="editAction(type)" class="edit"><i-->
                                        <!--class="fa fa-pencil-square-o fa-fw orange"></i></a></td>-->
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


                    <div class="modal fade ExchangeRate" tabindex="-1" id="add_new_pktuser_modal" role="dialog"
                         data-backdrop="static"
                         aria-labelledby="mySmallModalLabel">
                        <div class="modal-dialog modal-md">
                            <div class="modal-content checkout">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                            aria-hidden="true">&times;</span></button>
                                    <h4 id="pktuser" class="modal-title">Add Pkt User</h4>
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-md-12 col-sm-12">
                                            <form class="form-horizontal" name="mail">
                                                <fieldset>
                                                    <div class="form-group">
                                                        <input type="hidden" ng-model="setupobj.id" value="{{setupobj.id}}"/>
                                                        <label class="col-md-4 col-sm-4 control-label" for="">UserName</label>
                                                        <div class="col-md-6 col-sm-6" style="position:relative;">
                                                            <input id="name" name="name"
                                                                   type="text" placeholder="UserName"
                                                                   ng-model="username"
                                                                   class="form-control input-md" autocomplete="off">

                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-4 col-sm-4 control-label" for="">Password</label>
                                                        <div class="col-md-6 col-sm-6" style="position:relative;">
                                                            <input id="password" name="password"
                                                                   type="text" placeholder="Password"
                                                                   ng-model="password"
                                                                   class="form-control input-md" autocomplete="off">

                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-4 col-sm-4 control-label" for="">Email</label>
                                                        <div class="col-md-6 col-sm-6" style="position:relative;">
                                                            <input id="email" name="email"
                                                                   type="text" placeholder="Email"
                                                                   ng-model="email"
                                                                   class="form-control input-md" autocomplete="off">

                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-4 col-sm-4 control-label" for="">Application</label>
                                                        <div class="col-md-6 col-sm-6" style="position:relative;">
                                                            <select id="application" name="application"
                                                                   type="text"
                                                                   ng-model="application"
                                                                    ng-options="application.name as application.name for application in applicationlist"
                                                                   class="form-control input-md">
                                                                <option value="">Select</option>
                                                            </select>

                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-4 col-sm-4 control-label" for="">Roles</label>
                                                        <div class="col-md-6 col-sm-6" style="position:relative;">
                                                            <input id="roles" name="roles"
                                                                   type="text" placeholder="Roles"
                                                                   ng-model="roles"
                                                                   class="form-control input-md" autocomplete="off">

                                                        </div>
                                                    </div>

                                                </fieldset>

                                                <div class="modal-footer">
                                                    <button type="submit" class="btn btn-default" data-toggle="modal"
                                                            data-target=".print-me" ng-click="savePktUser()">save
                                                    </button>
                                                    <button type="button" class="btn btn-default"
                                                            ng-click="removeApplication()" data-dismiss="modal">close
                                                    </button>

                                                </div>

                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>
    </div>
</div>
</body>
</html>

