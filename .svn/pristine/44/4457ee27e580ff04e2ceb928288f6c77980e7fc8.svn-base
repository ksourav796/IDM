package com.hyva.idm.sass.sasspojo;

import lombok.Data;

import java.util.List;

@Data
public class MenuPojo {

    private String keySubscription;
    private String menuName;
    private String sequenceno;
    private String ParentId;
    private String tableRequired;
    private String ParentValue;
    private String pageRequired;
    List<MenuPojo> sunMenuList;

    public String getKeySubscription() {
        return keySubscription;
    }

    public void setKeySubscription(String keySubscription) {
        this.keySubscription = keySubscription;
    }

    public String getMenuName() {
        return menuName;
    }

    public void setMenuName(String menuName) {
        this.menuName = menuName;
    }

    public String getSequenceno() {
        return sequenceno;
    }

    public void setSequenceno(String sequenceno) {
        this.sequenceno = sequenceno;
    }

    public String getParentId() {
        return ParentId;
    }

    public void setParentId(String parentId) {
        ParentId = parentId;
    }

    public String getTableRequired() {
        return tableRequired;
    }

    public void setTableRequired(String tableRequired) {
        this.tableRequired = tableRequired;
    }

    public String getParentValue() {
        return ParentValue;
    }

    public void setParentValue(String parentValue) {
        ParentValue = parentValue;
    }

    public String getPageRequired() {
        return pageRequired;
    }

    public void setPageRequired(String pageRequired) {
        this.pageRequired = pageRequired;
    }

    public List<MenuPojo> getSunMenuList() {
        return sunMenuList;
    }

    public void setSunMenuList(List<MenuPojo> sunMenuList) {
        this.sunMenuList = sunMenuList;
    }
}
