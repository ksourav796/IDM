                <h4 id="cust-notif" class="modal-title"></h4>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <form>
                            <table class="table table-striped table-bordered">
                                <thead>
                                <tr>
                                    <!--<th>Serial No</th>-->
                                    <th>From Table Fields</th>
                                    <th>To Table</th>
                                    <th>To Table Fields</th>
                                    <th>Clear Row</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr ng-repeat="createtable1 in createtables1list ">

                                    <td>
                                        <select name="countryId" ng-model="createtable1.fromtablefields"
                                                ng-options="field.fieldName as field.fieldName for field in fromcolumnlist"
                                                        class="form-control input-md">
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td>
                                        <select name="countryId" ng-model="createtable1.totable"
                                                ng-options="table.keyName as table.keyValue for table in tableslist"
                                                        class="form-control input-md"
                                                        ng-change='getfromcolumnlist(createtable1.totable)'>
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td>
                                        <select name="countryId" ng-model="createtable1.totablefields"
                                                ng-options="field.fieldName as field.fieldName for field in fromcolumnlist"
                                                        class="form-control input-md">
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td> <button type="button" class="btn btn-default hi_btn"
                                                 ng-click="removeAddNewCreateTable()">X
                                    </button>
                                    </td>

                                </tr>
                                </tbody>
                            </table>
                            <td>
                                <button type="button"
                                        class="btn btn-primary hi_btn"
                                        ng-click="saveCreatedFields(createtables1list)">
                                    Save
                                </button>
                            </td>

                            <td>
                                <button type="button"
                                        class="btn btn-primary"
                                        ng-click="removeServices()"
                                        data-dismiss="modal">Close
                                </button>
                            </td>

                            <div class="form-group">
                                <input type="submit"
                                       class="btn btn-primary addnew pull-right"
                                       value="Add New" ng-click="addalltables()">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade ExchangeRate" tabindex="-1" id="add_new_Table_updatetable_modal" role="dialog"
     data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-md">
        <div class="modal-content checkout">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 id="cust-notif" class="modal-title">Update Table</h4>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <form>
                            <table class="table table-striped table-bordered">
                                <thead>
                                <tr>
                                    <th>From Table</th>
                                    <th style="word-break: break-word">From Column</th>
                                    <th style="word-break: break-word">FromCommonColumn</th>
                                    <th>ToTable</th>
                                    <th>ToColumn</th>
                                    <th>ToCommonColumn</th>
                                    <th>Operation</th>
                                    <th>Clear Row</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr ng-repeat="updatetable in updatetableslist ">

                                    <td>
                                        <input name="countryId" ng-model="updatetable.fromtable"
                                                class="form-control input-md" readonly >
                                        </input>
                                    </td>
                                    <td>
                                        <select name="countryId" ng-model="updatetable.fromcolumn"
                                                ng-options="field.fieldName as field.fieldName for field in fromcolumnlist"
                                                        class="form-control input-md">
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td>
                                        <select name="countryId" ng-model="updatetable.fromcommoncolumn"
                                                ng-options="field.fieldName as field.fieldName for field in fromcolumnlist"
                                                        class="form-control input-md">
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td>
                                        <select name="countryId" ng-model="updatetable.totable"
                                                ng-options="table.keyName as table.keyValue for table in tableslist"
                                                        class="form-control input-md" ng-change='getfromcolumnlist(updatetable.totable)'>
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td>
                                        <select name="countryId" ng-model="updatetable.tocolumn"
                                                ng-options="field.fieldName as field.fieldName for field in fromcolumnlist"
                                                        class="form-control input-md">
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td>
                                        <select name="countryId" ng-model="updatetable.tocommoncolumn"
                                                ng-options="field.fieldName as field.fieldName for field in fromcolumnlist"
                                                        class="form-control input-md">
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td>
                                        <select name="operation" ng-model="updatetable.operation"
                                                class="form-control input-md">
                                            <option value="">-Select-</option>
                                            <option value="increase">Increase</option>
                                            <option value="decrease">Decrease</option>
                                        </select>
                                    </td>
                                    <td> <button type="button" class="btn btn-default hi_btn"
                                                 ng-click="removeAddNewUpdate()">X
                                    </button>
                                    </td>

                                </tr>
                                </tbody>
                            </table>
                            <td>
                                <button type="button"
                                        class="btn btn-primary hi_btn"
                                        ng-click="saveUpdateTable(updatetableslist)">
                                    Save
                                </button>
                            </td>

                            <td>
                                <button type="button"
                                        class="btn btn-primary"
                                        ng-click="removeServices()"
                                        data-dismiss="modal">Close
                                </button>
                            </td>

                            <div class="form-group">
                                <input type="submit"
                                       class="btn btn-primary addnew pull-right"
                                       value="Add New" ng-click="addupdatetable()">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>