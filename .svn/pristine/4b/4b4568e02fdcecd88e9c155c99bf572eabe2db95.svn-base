<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/html">

<body>
<div class="container-fluid">
    <div class="container cus_main_row">
        <div class="row">
            <h2 class="pos-header-new">
                <div>Table</div>
            </h2>
        </div>
        <div class="row inner_row_bg">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="panel">
                    <div class="panel-body" style="height: 65px;">
                        <form class="form-horizontal ng-pristine ng-valid">
                        <div class="col-md-3 col-lg-3 col-sm-5 col-xs-5">

                                <div class="form-group">
                                    <div class="col-sm-12">
                                        <p class="input-group" style="display: block;">
                                            <input id="search_grade" name="textinput"  type="text" autocomplete="off"
                                                   placeholder="search Application" ng-model="searchText" class="form-control input-md"
                                                   ng-keyup="$event.keyCode == 13 ?getpaginatedTableList(searchText):null"/>
                                        </p>
                                    </div>

                                </div>

                        </div>

                        <div class="col-md-3 col-lg-3 col-sm-5 col-xs-5">
                            <div class="col-sm-12">
                                <p class="input-group" align="right" style="display: block;">
                                    <input id="searchh" name="textinput"  type="text" autocomplete="off"
                                           placeholder="search Table Code" ng-model="searchText1" class="form-control input-md"
                                           ng-keyup="$event.keyCode == 13 ?getpaginatedTableList(searchText1):null"/>
                                </p>
                            </div>
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-5 col-xs-5">
                            <div class="col-sm-12">
                                <p class="input-group" align="right" style="display: block;">
                                    <input id="search" name="textinput"  type="text" autocomplete="off"
                                           placeholder="search Table Name" ng-model="searchText2" class="form-control input-md"
                                           ng-keyup="$event.keyCode == 13 ?getpaginatedTableList(searchText2):null"/>
                                </p>
                            </div>
                        </div>
                        <div class="col-md-3 col-lg-3 col-sm-5 col-xs-5">
                                <div class="col-sm-12">
                                    <button id="" type="button" class="btn btn-default hi_btn"
                                            ng-click="addNewPkt()">Add
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="fixed-table-container">
                            <div class="table_wrap fixed-table-container-inner">
                                <div class="header-background"></div>
                                <table data-toggle="table" data-cache="false" data-height="299"
                                       style="margin-bottom: 0px;"
                                       class="hi_table table table-hover table-bordered table-striped table-fixed-header">
                                    <thead>
                                    <tr>

                                        <th data-field="formRegno">
                                            <div class="th-inner">Application</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Table Code</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Table Name </div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner"> Descriptor</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Status</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Edit</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Search</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Create Table</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Update Table</div>
                                        </th>
                                        <th data-field="formRegno">
                                            <div class="th-inner">Plus Key</div>
                                        </th>


                                    <tbody id="parentTable">
                                    <tr ng-repeat="type in permissionList">
                                        <td>{{type.keySubscription}}</td>
                                        <td>{{type.keyName}}</td>
                                        <td>{{type.keyValue}}</td>
                                        <td>{{type.descriptor}}</td>
                                        <td>{{type.status}}</td>
                                        <!--<td>{{type.status}}</td>-->
                                        <td><a id="edit_{{$index}}" href="javascript:void(0)"
                                               ng-click="editTable(type)" class="edit"><i
                                                class="fa fa-pencil-square-o fa-fw orange"></i></a></td>
                                                <td><button id="searchrow" type="button" class="btn btn-default hi_btn"
                                                            ng-click="searchrow(type)">Search
                                                </button>
                                                </td>
                                        <td><button id="create" type="button" class="btn btn-default hi_btn"
                                                            ng-click="createtable(type)">Create
                                                </button>
                                                </td> <td><button id="update" type="button" class="btn btn-default hi_btn"
                                                            ng-click="updatetable(type)">Update
                                                </button>
                                                </td>
                                        <td>
                                            <button id="plus" type="button" class="btn btn-default hi_btn"
                                                    ng-click="pluskey(type)">Plus Key
                                            </button>
                                        </td>
                                        <!--</tr>-->
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-5 col-md-7 col-sm-10 pagination_" style="float: right; text-align: right; margin-top: 10px;">

                        <button type="button" class="btn btn-default hi_btn" ng-class="{'diabled-class': first}" ng-disabled="first"
                                ng-click="getpaginatedTableList('firstPage')"  ng-model="first">First
                        </button>
                        <button type="button" class="btn btn-default hi_btn"  ng-class="{'diabled-class': prev}" ng-disabled="prev"
                                ng-click="getpaginatedTableList('prevPage')"   ng-model="prev">Prev
                        </button>
                        <button type="button" class="btn btn-default hi_btn"  name="next" ng-disabled="next"
                                ng-click="getpaginatedTableList('nextPage')"  value="next"  ng-class="{'diabled-class': next}" ng-model="next">Next
                        </button>
                        <button type="button" class="btn btn-default hi_btn"
                                ng-click="getpaginatedTableList('lastPage')"   ng-class="{'diabled-class': last}" ng-disabled="last"
                                ng-model="last">Last
                        </button>
                    </div>
                </div>
            </div>
