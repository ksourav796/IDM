                                        <select name="countryId" ng-model="pluskey.columnName"
                                                ng-options="pktfields.fieldName as pktfields.fieldName for pktfields in pluskeylist1"
                                                class="form-control input-md">
                                            <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td> <button type="button" class="btn btn-default hi_btn">
                                     </button>
                                    </td>


                                </tr>
                                </tbody>
                            </table>
                            <td>
                                <button type="button"
                                        class="btn btn-primary hi_btn"
                                        ng-click="saveplus(pluskeylist)">
                                    Save
                                </button>
                            </td>

                            <td>
                                <button type="button"
                                        class="btn btn-primary"
                                        ng-click="removeServices()"
                                        data-dismiss="modal">Close
                                </button>
                            </td>

                            <div class="form-group">
                                <input type="submit"
                                       class="btn btn-primary addnew pull-right"
                                       value="Add New" ng-click="adnewpluskey()">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade ExchangeRate" tabindex="-1" id="add_new_Table_createtable_modal" role="dialog"
     data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-md">
        <div class="modal-content checkout">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 id="cust-notif" class="modal-title">Create Table</h4>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <form>
                            <table class="table table-striped table-bordered">
                                <thead>
                                <tr>
                                    <!--<th>Serial No</th>-->
                                    <th>KeyName</th>
                                    <th>Operation</th>
                                    <th>Clear Row</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr ng-repeat="createtable in createtableslist ">
                                    <td>
                                        <select name="countryId" ng-model="createtable.keyname"
                                                ng-options="table.keyName as table.keyValue for table in tableslist"
                                                        class="form-control input-md"
                                                ng-change='getfromcolumnlist(createtable.keyname)'>
                                                    <option value="">-Select-</option>
                                        </select>
                                    </td>
                                    <td>
                                        <input name="countryId" ng-model="createtable.operation"
                                                        class="form-control input-md"
                                                        ng-click="opentablelist()">
                                        </input>
                                    </td>
                                    <td> <button type="button" class="btn btn-default hi_btn"
                                                 ng-click="removeaddnew()">X
                                    </button>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                            <td>
                                <button type="button"
                                        class="btn btn-primary hi_btn"
                                        ng-click="savekeyoperation(createtableslist)">
                                    Save
                                </button>
                            </td>

                            <td>
                                <button type="button"
                                        class="btn btn-primary"
                                        ng-click="removeServices()"
                                        data-dismiss="modal">Close
                                </button>
                            </td>

                            <div class="form-group">
                                <input type="submit"
                                       class="btn btn-primary addnew pull-right"
                                       value="Add New" ng-click="addcreatetable()">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade ExchangeRate" tabindex="-1" id="add_new_Table_createtable1_modal" role="dialog"
     data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-md">
        <div class="modal-content checkout">
            <div class="modal-header">
